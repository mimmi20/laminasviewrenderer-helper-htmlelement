{
  "name": "mimmi20/laminasviewrenderer-helper-htmlelement",
  "description": "Provides a render helper for Laminas View",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/laminasviewrenderer-helper-htmlelement",
  "support": {
    "issues": "https://github.com/mimmi20/laminasviewrenderer-helper-htmlelement/issues",
    "source": "https://github.com/mimmi20/laminasviewrenderer-helper-htmlelement"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
    "ext-mbstring": "*",
    "laminas/laminas-json": "^3.6.0",
    "laminas/laminas-view": "^2.35.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-intl": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.27.11 || ^0.28.1",
    "laminas/laminas-config": "^3.9.0",
    "laminas/laminas-escaper": "^2.13.0",
    "laminas/laminas-modulemanager": "^2.16.0",
    "laminas/laminas-servicemanager": "^3.22.1",
    "mimmi20/coding-standard": "^5.2.36",
    "nikic/php-parser": "^4.19.1 || ^5.0.2",
    "phpstan/extension-installer": "^1.4.1",
    "phpstan/phpstan": "^1.11.7",
    "phpstan/phpstan-deprecation-rules": "^1.2.0",
    "phpstan/phpstan-phpunit": "^1.4.0",
    "phpunit/phpunit": "^10.5.26",
    "psr/container": "^1.1.2 || ^2.0.2",
    "rector/rector": "^1.2.0",
    "rector/type-perfect": "^0.1.8",
    "symplify/phpstan-rules": "^13.0.0",
    "tomasvotruba/cognitive-complexity": "^0.2.3",
    "tomasvotruba/type-coverage": "^0.3.1",
    "tomasvotruba/unused-public": "^0.3.9"
  },
  "conflict": {
    "container-interop/container-interop": "<1.2.0"
  },
  "suggest": {
    "ext-intl": "required to use the Translator",
    "laminas/laminas-config": "to provide configuration (optional, as arrays and Traversables are also allowed)",
    "laminas/laminas-servicemanager": "to use the helper factories"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Mimmi20\\LaminasView\\Helper\\HtmlElement\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\LaminasView\\Helper\\HtmlElement\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\LaminasView\\Helper\\HtmlElement",
      "config-provider": "Mimmi20\\LaminasView\\Helper\\HtmlElement\\ConfigProvider"
    }
  }
}
