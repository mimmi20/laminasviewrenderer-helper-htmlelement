{
  "name": "mimmi20/laminasviewrenderer-helper-htmlelement",
  "description": "Provides a render helper for Laminas View",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/laminasviewrenderer-helper-htmlelement",
  "support": {
    "issues": "https://github.com/mimmi20/laminasviewrenderer-helper-htmlelement/issues",
    "source": "https://github.com/mimmi20/laminasviewrenderer-helper-htmlelement"
  },
  "require": {
    "php": "^8.1.0",
    "ext-mbstring": "*",
    "laminas/laminas-json": "^3.5.0",
    "laminas/laminas-view": "^2.27.0"
  },
  "require-dev": {
    "infection/infection": "^0.26.20",
    "laminas/laminas-config": "^3.8.0",
    "laminas/laminas-escaper": "^2.12.0",
    "laminas/laminas-modulemanager": "^2.14.0",
    "laminas/laminas-servicemanager": "^3.20.0",
    "mimmi20/coding-standard": "^4.0.5",
    "phpstan/extension-installer": "^1.2.0",
    "phpstan/phpstan": "^1.10.13",
    "phpstan/phpstan-deprecation-rules": "^1.1.3",
    "phpstan/phpstan-phpunit": "^1.3.11",
    "phpunit/phpunit": "^10.1.0"
  },
  "conflict": {
    "container-interop/container-interop": "<1.2.0"
  },
  "suggest": {
    "ext-intl": "required to use the Translator",
    "laminas/laminas-config": "to provide configuration (optional, as arrays and Traversables are also allowed)",
    "laminas/laminas-servicemanager": "to use the helper factories"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Mimmi20\\LaminasView\\Helper\\HtmlElement\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\LaminasView\\Helper\\HtmlElement\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "lock": false,
    "sort-packages": true,
    "allow-plugins": {
      "infection/extension-installer": true,
      "laminas/laminas-dependency-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\LaminasView\\Helper\\HtmlElement",
      "config-provider": "Mimmi20\\LaminasView\\Helper\\HtmlElement\\ConfigProvider"
    }
  }
}
